{
  "name": "@smallstack/markdown-component",
  "version": "0.1.0",
  "description": "Markdown Component for the smallstack framework",
  "author": "smallstack GmbH",
  "main": "./dist/bundle/index.umd.js",
  "types": "./dist/index.d.ts",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf dist",
    "compile": "tsc && rollup -c",
