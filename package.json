      },
      "web": {
        "entrypoint": "./dist/bundle/index.umd.js"
      }
    }
  },
  "dependencies": {
    "@smallstack/core-client": "*",
    "@smallstack/core-common": "*",
    "@smallstack/nativescript": "*",
    "marked": "^0.3.6",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "rimraf": "^2.6.1",
    "rollup": "^0.45.1",
    "rollup-plugin-html": "^0.2.1",
    "typescript": "^2.4.1"
  }
}
